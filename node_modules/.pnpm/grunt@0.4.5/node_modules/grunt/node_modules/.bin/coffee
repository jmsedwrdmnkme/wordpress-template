#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/james/Sites/blankslate/node_modules/.pnpm/coffee-script@1.3.3/node_modules/coffee-script/bin/node_modules:/home/james/Sites/blankslate/node_modules/.pnpm/coffee-script@1.3.3/node_modules/coffee-script/node_modules:/home/james/Sites/blankslate/node_modules/.pnpm/coffee-script@1.3.3/node_modules:/home/james/Sites/blankslate/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/james/Sites/blankslate/node_modules/.pnpm/coffee-script@1.3.3/node_modules/coffee-script/bin/node_modules:/home/james/Sites/blankslate/node_modules/.pnpm/coffee-script@1.3.3/node_modules/coffee-script/node_modules:/home/james/Sites/blankslate/node_modules/.pnpm/coffee-script@1.3.3/node_modules:/home/james/Sites/blankslate/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../coffee-script@1.3.3/node_modules/coffee-script/bin/coffee" "$@"
else
  exec node  "$basedir/../../../../../coffee-script@1.3.3/node_modules/coffee-script/bin/coffee" "$@"
fi
